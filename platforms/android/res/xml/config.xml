<?xml version='1.0' encoding='utf-8'?>
<widget id="com.vtayur.madhvanama" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <feature name="File">
        <param name="android-package" value="org.apache.cordova.file.FileUtils" />
        <param name="onload" value="true" />
    </feature>
    <allow-navigation href="cdvfile:*" />
    <feature name="Media">
        <param name="android-package" value="org.apache.cordova.media.AudioHandler" />
    </feature>
    <name>Madhvanama</name>
    <description>
    </description>
    <author email="varuntayur@gmail.com" href="http://varuntayur.wordpress.com">
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="market:*" />
    <icon density="ldpi" src="res/icon36.png" />
    <icon density="mdpi" src="res/icon48.png" />
    <icon density="hdpi" src="res/icon72.png" />
    <icon density="xhdpi" src="res/icon96.png" />
    <icon density="xxhdpi" src="res/icon144.png" />
    <preference name="loglevel" value="DEBUG" />
</widget>
